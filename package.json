{
	"name": "tamachi",
	"version": "1.0.1",
	"description": "Tamachi WordPress Theme",
	"homepage": "https://github.com/garciaalvaro/tamachi",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js && rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js && rm _temp.js",
		"pragma": "webpack --mode production --config ./webpack/pragma.config.babel.js && rm _release/_temp.js",
		"release": "rm -rf build _release && npm run type && npm run version && npm run prod && npm run copy && npm run pragma",
		"type": "tsc"
	},
	"dependencies": {
		"dompurify": "^2.0.7",
		"immer": "^5.1.0",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"simplebar-react": "^2.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.7.7",
		"@babel/preset-env": "^7.7.7",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-typescript": "^7.7.7",
		"@babel/register": "^7.7.7",
		"@types/dompurify": "2.0.0",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.1.0",
		"@types/react-dom": "^16.9.4",
		"@types/wordpress__api-fetch": "^3.2.2",
		"@types/wordpress__dom-ready": "^2.3.0",
		"@types/wordpress__i18n": "^3.4.0",
		"@types/wordpress__url": "^2.3.0",
		"babel-loader": "^8.0.6",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.0",
		"file-loader": "^5.0.2",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.7.4",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10",
		"webpack-loader-clean-pragma": "^1.0.2"
	}
}
